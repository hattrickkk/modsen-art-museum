{
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "react/react-in-jsx-scope": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto",
            },
        ],
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020,
    },
}
